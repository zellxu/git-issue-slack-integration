<% if @result %>
  $("#repo-modal").modal('hide')
  $("#repos-<%= repo.id %>").replaceWith("<%= j render partial: 'row', locals: {r: repo} %>")
  $("#repos-<%= repo.id %> .panel-heading").effect( "highlight", 3000)
  $("#flash-message").html("<%= j render partial: 'layouts/flash_message', locals: {flash: @flash_message} %>")
  $('html, body').animate({ scrollTop: 0 }, 'fast')
<% else %>
  $("#repo-modal").html("<%= j render partial 'modal', locals: {edit: true} %>")
<% end %>